{
  "name": "hash-compare",
  "version": "1.0.0",
  "description": "A package for hashing and comparing objects with additional functionality for version tracking and patch generation.",
  "keywords": [
    "hashing",
    "object",
    "comparison",
    "diff",
    "patch",
    "merge",
    "versioning"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "minify": "terser dist/index.js --compress --mangle --output dist/index.js",
    "build": "tsc && npm run minify",
    "prepublishOnly": "npm run build"
  },
  "author": "Ramesh Desai",
  "license": "MIT",
  "devDependencies": {
    "terser": "^5.33.0",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "util": "^0.12.5"
  },
  "files": [
    "dist"
  ]
}